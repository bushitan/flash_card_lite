}
.icon{
    /* vertical-align: middle; */
    width:100%; 
    height:100%;
}

.cell__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
    line-height: 22px;
}
.cell__ft {
  text-align: right;
  color: #999999;
}
.cell__ft_in-access {
  padding-right: 13px;
  position: relative;
}
.cell__ft_in-access:after {
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #C8C8CD;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
          transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: relative;
  top: -2px;
  position: absolute;
  top: 50%;
  margin-top: -4px;
  right: 2px;
}

